{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "factoryName": { "type": "string" },
    "environment": { "type": "string" },
    "sqlMiConnectionString": { "type": "securestring", "defaultValue": "" },
    "sqlMiKeyVaultBaseUrl": { "type": "string", "defaultValue": "" },
    "sqlMiKeyVaultSecretName": { "type": "string", "defaultValue": "" },
    "blobStorageConnectionString": { "type": "securestring", "defaultValue": "" },
    "blobKeyVaultBaseUrl": { "type": "string", "defaultValue": "" },
    "blobKeyVaultSecretName": { "type": "string", "defaultValue": "" },
    "container": { "type": "string" },
    "baseFolder": { "type": "string" },
    "containerUri": { "type": "string" },
    "containerSasToken": { "type": "string" }
  },
  "resources": [
    {
      "name": "ADF_ArmTemplate_0",
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2024-03-01",
      "properties": {
        "mode": "incremental",
        "templateLink": {
          "uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
          "factoryName": { "value": "[parameters('factoryName')]" },
          "environment": { "value": "[parameters('environment')]" },
          "sqlMiConnectionString": { "value": "[parameters('sqlMiConnectionString')]" },
          "sqlMiKeyVaultBaseUrl": { "value": "[parameters('sqlMiKeyVaultBaseUrl')]" },
          "sqlMiKeyVaultSecretName": { "value": "[parameters('sqlMiKeyVaultSecretName')]" },
          "blobStorageConnectionString": { "value": "[parameters('blobStorageConnectionString')]" },
          "blobKeyVaultBaseUrl": { "value": "[parameters('blobKeyVaultBaseUrl')]" },
          "blobKeyVaultSecretName": { "value": "[parameters('blobKeyVaultSecretName')]" },
          "container": { "value": "[parameters('container')]" },
          "baseFolder": { "value": "[parameters('baseFolder')]" }
        }
      }
    }
  ]
}
